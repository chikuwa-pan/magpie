<div class="wrapper form">
  <h2 class="title flex flex_center">Log in
    <span class="material-symbols-rounded">login</span>
  </h2>

  <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>

    <div class="field">
      <%= f.label :email, class:"field_title" %><br />
      <%= f.email_field :email, autofocus: true, autocomplete: "email", class:"form_input" %>
    </div>

    <div class="field">
      <%= f.label :password, class:"field_title" %><br />
      <%= f.password_field :password, autocomplete: "current-password", class:"form_input" %>
    </div>

    <% if devise_mapping.rememberable? %>
      <div class="field">
        <%= f.check_box :remember_me %>
        <%= f.label :remember_me %>
      </div>
    <% end %>

    <div class="item_btn">
      <%= f.submit "Log in", class:"form_input", id:"submit" %>
    </div>
  <% end %>

  <%#= render "devise/shared/links" %>
</div>
